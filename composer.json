{
  "name": "drewdan/asset-handler",
  "description": "A companion package for the Prometheus Markdown npm package. This will provide a route to upload assets and return their url",
  "license": "MIT",
  "require": {
    "php": "^7.4|^8.0",
    "laravel/framework": "^8.0."
  },
  "authors": [
    {
      "name": "Andrew Arscott",
      "email": "andrew@prometheuscomputing.co.uk"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Drewdan\\AssetHandler\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Drewdan\\AssetHandler\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Drewdan\\AssetHandler\\AssetHandlerServiceProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "orchestra/testbench": "^6.0",
    "ext-json": "*"
  },
  "scripts": {
    "test": "phpunit"
  }
}
